footer-height = 350px


.footerModes
	text-align center
	width 100%
	height 100%

.footerWrapper .footerItem
	width 33.33%
	float left
	height 100%

.footerWrapper .footerItem .desc
	height 35px

.footerWrapper .receiveAlerts, .footerWrapper .analysisAlerts, .footerWrapper .submitStory
	width 100%	
	height 100%
	
.footerWrapper .analysisAlerts .icon {
	background-image url('../images/home-icons.png')
	background-position 0px -400px
	background-repeat no-repeat
	height 92px
	margin 20px auto
	width 150px
}

.footerWrapper .submitStory .icon	
	background-image url('../images/home-icons.png')
	background-position 0px -540px
	background-repeat no-repeat
	height 92px
	margin 20px auto
	width 134px

.footerWrapper .receiveAlerts .icon
	background-image url('../images/home-icons.png')
	background-position 0px -740px
	background-repeat no-repeat
	height 92px
	margin 20px auto
	width 122px

.footerModes .title
	font-size 29px
	font-family "Georgia",serif
	margin 0px 0px 20px


.footerModes .desc
	font-size 15px
	line-height 1.2
	color #666
	margin 0px 0px 20px

.footerModes .action
	text-transform uppercase
	background none repeat scroll 0% 0% #9BB251
	padding 14px 34px
	border-radius 2px
	border 1px solid #839C26
	font-size 11px
	font-weight bold
	cursor pointer
	color #FFF
	text-decoration none

.footerModesContainer
	width 100%
	height 300px
	background-color #222

.footerShareContainer
	//background-color #464352
	width 100%
	height footer-height

.footerWrapper
	width 100%
	max-width 960px
	margin auto
	color #FFF
	height 100%

.footerMovableWrapper
	width 100%
	height footer-height	
	background-color #464352
	color #FFF
	clear both
	bottom 0

.fb-share-button
	bottom 3px
	left 10px

.alert-dialog-box .submit-button
	text-align center
	margin 10px 0

.alert-dialog-box .submit-button button
	border-radius 5px
	font-size 12px

.footerMovableModes
	height footer-height
	max-width 960px
	width 100%
	margin auto
	background-color #464352
	
	a
		color #FFF
		font-size 12px


	.container-large
		width 365px
		height 100%
		padding 0px
		float left

	.container-large>div
		padding 10px

	.container-small
		width 200px
		height 100%	
		padding 0px
		float left

	.container-small>div
		padding 10px


	.about-project
		color #B1AFB6
		font-size 12px

	.title
		font-weight bold		

	.wri-address
		color #B1AFB6
		font-size 12px

	.wri-address .title
		font-weight bold

	.wri-address .wri-logo
		font-weight bold
		background url('../images/wri.png') no-repeat scroll 0% 0% transparent
		height 31px
		width 269px
		display block
		margin 10px 0px



	.share
		color #FFF

	.share .title
		font-family fira_sans_otmedium
		font-weight 200

	.share-container
		padding 10px 20px
		border-radius 160px
		background-color #FFF
		margin 10px 0px


	.sign-up
		margin 10px 0px

	.subscribe
		margin 0px 5px


	.gfw-logo
		background url('../images/monochromatic_gfw.jpg') no-repeat scroll 0% 0% transparent
		text-align center
		height 71px
		display block
		width 122px
		margin 10px

	.footer-logo
		background url('../images/logos-from-gfw.png') no-repeat scroll 0% 0% transparent
		display inline-block
		vertical-align middle
		margin 0px 38px 38px 0px
		width 88px

	.footer-logo.digital-globe
			background url('../images/dg-globe.png') no-repeat scroll 0% 0% transparent		
			background-position 0px 0px
			height 28px

	.footer-logo.esri
			background-position 0px -654px
			height 28px

	.footer-logo.google
			background-position 0px -734px
			height 52px

	.footer-logo.bpredd		
			background url('../images/bpredd.png') no-repeat scroll 0% 0% transparent
			background-position 0px 0px
			height 57px
			
