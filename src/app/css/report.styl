html, body
	height 100%
	width 100%
	padding 0
	margin 0

.report-page
	font-family Arial, Georgia, "Helvetica Neue", Helvetica, sans-serif

.report-title
	font-family "Arial Narrow", Arial, sans-serif
	margin 30px 0 50px

.report-title, .section-title
	text-align center
	font-weight bold
	font-size 28px

.section-title
	font-size 22px
	margin 10px 0

.map-container
	border 2px solid black
	margin 0 auto
	height 700px
	width 80%

.map
	height 100%
	width 100%

@media print
	.page-break
		display block 
		page-break-before always