@font-face
	font-family "Arial MT Condensed Bold"
	src url("../fonts/ArialMTStd-BoldCond.otf") format("opentype")

@font-face
	font-family "Arial MT Condensed Light"
	src url("../fonts/ArialMTStd-LightCond.otf") format("opentype")

html, body
	height 100%
	width 100%
	padding 0
	margin 0

.report-page
	font-family Arial, Georgia, "Helvetica Neue", Helvetica, sans-serif

.report-title
	font-family "Arial MT Condensed Bold", Arial, Georgia, sans-serif
	margin 30px 0 50px

.report-title, .section-title
	text-align center
	font-size 28px

.section-title
	font-family "Arial MT Condensed Light", Arial, Georgia, sans-serif
	font-size 22px
	margin 10px 0

.map-container
	border 2px solid black
	margin 0 auto
	height 500px
	width 650px

.map
	height 100%
	width 100%

@media print
	.page-break
		display block 
		page-break-before always